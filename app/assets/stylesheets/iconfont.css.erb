@font-face {
	font-family: 'grapefruit';
	src:url('<%= asset_path("assets/grapefruit.eot?") + "lqfn5p" %>');
	src:url('<%= asset_path("assets/grapefruit.eot?") + "#iefixlqfn5p" %>') format('embedded-opentype'),
		url('<%= asset_path("assets/grapefruit.woff?") + "lqfn5p" %>') format('woff'),
		url('<%= asset_path("assets/grapefruit.ttf?") + "lqfn5p" %>') format('truetype'),
		url('<%= asset_path("assets/grapefruit.svg?") + "lqfn5p#grapefruit" %>') format('svg');
	font-weight: normal;
	font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
	font-family: 'grapefruit';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

